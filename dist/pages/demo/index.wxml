    <!--modal的演示-->
    <view class="wk-panel top-gap">
        <view class="panel-header">组件的使用方法</view>
        
    <view class="agent">
        <image src="{{ $agent$data.headPortraitUrl }}?x-oss-process=image/resize,w_120" mode="aspectFill" lazy-load="true"/>
        <view class="tel" bindtap="$agent$call">
            <text class="iconfont icon-tel"/>
            <text class="memo">电话咨询</text>
        </view>
        <view class="wechat" bindtap="$agent$addWechat">
            <text class="iconfont icon-wechat"/>
            <text class="memo">添加微信</text>
        </view>        
        <view class="summary">
            <text class="name">{{ $agent$data.agentName }}</text>
            <text class="seniority">加入悟空：{{ $agent$data.seniority }}年</text>
        </view>        
    </view>

