.wk-modal{background-color:#fff;box-shadow:0 8rpx 46rpx 10rpx rgba(0,0,0,0.2),0 4rpx 12rpx rgba(0,0,0,0.15);-webkit-border-radius:6rpx;border-radius:6rpx;display:none;min-width:340rpx;position:absolute;max-width:60%;z-index:9998}.wk-modal .modal-header{font-size:32rpx;line-height:60rpx;padding:30rpx 0 15rpx;text-align:center}.wk-modal .modal-body{color:#999;font-size:28rpx;line-height:150%;padding:0 15rpx;text-align:center}.wk-modal .modal-footer{border-top:1rpx solid #e4e4e4;margin-top:30rpx;text-align:center}.wk-modal .modal-footer .wk-btn{border-left:1rpx solid #e4e4e4}.wk-modal .modal-footer .wk-btn:nth-child(1){border-left:none}.wk-modal-mask{background-color:rgba(0,0,0,0.65);display:none;height:100%;left:0;position:fixed;top:0;width:100%;z-index:9997}